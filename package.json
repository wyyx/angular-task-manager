{
  "name": "angular-task-manager",
  "version": "0.0.0",
  "scripts": {
    "start": "yarn install --production=false && yarn run build && yarn run install-server",
    "install-server": "cd ./server && yarn install && cd ..",
    "ng": "ng",
    "hmr": "ng serve --configuration hmr",
    "build": "node --max_old_space_size=512 ./node_modules/@angular/cli/bin/ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ng-server": "ng serve",
    "server": "json-server ./mock/data.json --watch",
    "dev": "concurrently \"npm run ng-server\" \"npm run server\"",
    "dev-hmr": "concurrently \"npm run hmr\" \"npm run server\"",
    "validate-ts": "tsc -w --noEmit"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.2",
    "@angular/cdk": "^7.2.2",
    "@angular/common": "^7.2.2",
    "@angular/compiler": "^7.2.2",
    "@angular/core": "^7.2.2",
    "@angular/forms": "^7.2.2",
    "@angular/http": "^7.2.2",
    "@angular/material": "^7.2.2",
    "@angular/platform-browser": "^7.2.2",
    "@angular/platform-browser-dynamic": "^7.2.2",
    "@angular/router": "^7.2.2",
    "@ng-bootstrap/ng-bootstrap": "^4.0.2",
    "@ngrx/effects": "^7.0.0",
    "@ngrx/entity": "^7.1.0",
    "@ngrx/router-store": "^7.0.0",
    "@ngrx/store": "^7.0.0",
    "@ngrx/store-devtools": "^7.0.0",
    "angular-calendar": "^0.26.6",
    "angularx-flatpickr": "^6.1.0",
    "bootstrap": "^4.2.1",
    "core-js": "^2.5.4",
    "date-fns": "^1.30.1",
    "flatpickr": "^4.5.2",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.11",
    "ngx-contextmenu": "^5.1.1",
    "rxjs": "^6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular/cli": "~7.2.3",
    "@angular/compiler-cli": "^7.2.2",
    "@angular/language-service": "^7.2.2",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.119",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "concurrently": "^4.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.14.0",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "material-design-icons-iconfont": "^3.0.3",
    "ngrx-store-freeze": "^0.2.4",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.2.4"
  }
}
